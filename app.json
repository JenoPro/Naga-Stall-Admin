{
  "expo": {
    "name": "Naga-Stall-Admin",
    "slug": "Naga-Stall-Admin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSCameraUsageDescription": "This app uses the camera for live streaming raffle events to participants."
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow Naga-Stall-Admin to access your camera for live streaming raffle events.",
          "microphonePermission": "Allow Naga-Stall-Admin to access your microphone for live streaming audio."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "e33b3ed4-5fb8-40d8-a545-dd9d4069f164"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e33b3ed4-5fb8-40d8-a545-dd9d4069f164"
    }
  }
}